{! This component can accept an option content flag !}

<section{~s}
  {?data.content-flag}
    id="content"
  {/data.content-flag}
  {#data.display}
    {>"components__{.name}" data=data /}
  {/data.display}
>
  <div class="container">
    {>"components__partials__component-renderer" components=data.components /}
  </div>
</section>
